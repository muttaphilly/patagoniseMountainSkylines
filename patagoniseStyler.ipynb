   "source": [
    "# Patagonise Mountain Skylines"
   ]
  },
{
   "cell_type": "code",
   "execution_count": null,
   "id": "db053ffb",
   "metadata": {},
   "outputs": [],
   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "import os\n",
    "from scipy.signal import find_peaks\n",
    "from scipy.interpolate import interp1d\n",
    "import matplotlib.pyplot as plt\n",
    "from matplotlib.image import imread"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "f3fe0059",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Input image\n",
    "image_path = \"/Add/Your/Image/Pathname\"\n",
    "image = cv2.imread(image_path, cv2.IMREAD_GRAYSCALE)\n",
    "\n",
    "# Apply a threshold to separate sky from mountains\n",
    "_, binary = cv2.threshold(image, 127, 255, cv2.THRESH_BINARY)\n",
    "\n",
    "# Finds the skyline\n",
    "height, width = binary.shape\n",
    "skyline_points = []\n",
    "for col in range(width):\n",
    "    for row in range(height):\n",
    "        if binary[row, col] == 0:\n",
    "            skyline_points.append([col, row])\n",
    "            break\n",
    "\n",
    "# variable for the flowy style version\n",
    "skyline_points = np.array(skyline_points)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "8f7bbfae",
   "metadata": {},
   "source": [
    "# Stylised Version"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "820b83cb",
   "metadata": {},
   "outputs": [],
   "source": [
    "def create_flowly_mountain(points, width, height, peak_prominence=10, smoothness=0.2):\n",
    "    inverted_y = height - points[:, 1]\n",
    "    peaks, _ = find_peaks(inverted_y, prominence=peak_prominence, distance=20)\n",
    "    if len(peaks) < 2:\n",
    "        peaks = np.array([0, len(points)-1])\n",
    "    x = np.arange(width)\n",
    "    peak_points = np.column_stack((points[peaks, 0], inverted_y[peaks]))\n",
    "    f = interp1d(peak_points[:, 0], peak_points[:, 1], kind='linear', fill_value='extrapolate')\n",
    "    smooth_y = f(x)\n",
    "    blended_y = inverted_y * (1 - smoothness) + smooth_y * smoothness\n",
    "    return np.column_stack((x, height - blended_y.astype(int)))\n",
    "\n",
    "# Create skyline\n",
    "stylised_points = create_flowly_mountain(skyline_points, width, height, peak_prominence=5, smoothness=0.3)\n",
    "\n",
    "# Add a horizontal base\n",
    "bottom_y = np.max(stylised_points[:, 1])\n",
    "stylised_points[:, 1] = np.minimum(stylised_points[:, 1], bottom_y)\n",
    "\n",
    "# Create a canvas\n",
    "canvas = np.zeros((height, width), dtype=np.uint8)\n",
    "\n",
    "# Draw and fill\n",
    "cv2.fillPoly(canvas, [np.vstack((stylised_points, [[width-1, bottom_y], [0, bottom_y]]))], 255)\n",
    "\n",
    "# Set a location to save image\n",
    "output_folder = '/Add/Your/Proj/Folder'\n",
    "output_path = os.path.join(output_folder, 'flowly_mountain.jpg')\n",
    "# Make sure folder exists\n",
    "os.makedirs(output_folder, exist_ok=True)\n",
    "# Write file\n",
    "cv2.imwrite(output_path, canvas)\n",
    "\n",
    "print(f\"Flowly mountain image saved to: {output_path}\")"
   ]
  },
  {
