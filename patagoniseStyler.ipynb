   "metadata": {},
   "source": [
    "# Basic Skyline Extraction"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "6143f7b2",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Beef up the outline, making it more visible\n",
    "kernel = np.ones((3,3), np.uint8)\n",
    "skyline = cv2.dilate(skyline, kernel, iterations=1)\n",
    "\n",
    "# Set the output location\n",
    "output_folder = '/Add/Your/Proj/Folder'\n",
    "output_path = os.path.join(output_folder, 'skyline_outline.jpg')\n",
    "# Check the folder lives\n",
    "os.makedirs(output_folder, exist_ok=True)\n",
    "# Write file\n",
    "cv2.imwrite(output_path, skyline)\n",
    "\n",
    "print(f\"Skyline outline saved to: {output_path}\")"
   ]
  },
